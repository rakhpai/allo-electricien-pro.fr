{
  "name": "allo-electricien-pro",
  "version": "1.0.0",
  "type": "module",
  "description": "410 électriciens urgence IDF + Oise - Allo Électricien",
  "main": "index.js",
  "scripts": {
    "data": "node scripts/generate-data.js",
    "content": "node scripts/generate-content.js",
    "profiles-export": "node scripts/export-electricien-profiles.cjs",
    "video-export": "node scripts/export-videos-from-supabase.js",
    "video-sitemap": "node scripts/generate-video-sitemap.cjs",
    "sitemap-index": "node scripts/generate-sitemap-index.cjs",
    "pre-deploy": "node scripts/pre-deploy-check.cjs",
    "post-deploy": "node scripts/ping-search-engines.cjs",
    "build:css": "tailwindcss -i ./static/css/tailwind-input.css -o ./static/css/tailwind.min.css --minify",
    "build:css:watch": "tailwindcss -i ./static/css/tailwind-input.css -o ./static/css/tailwind.min.css --watch",
    "build": "npm run build:css && npm run profiles-export && npm run video-sitemap && npm run sitemap-index && hugo --minify",
    "serve": "hugo server -D",
    "deploy": "npm run pre-deploy && bash scripts/deploy-local.sh && npm run post-deploy",
    "full": "npm run data && npm run map-electricians && npm run profiles-export && npm run video-export && npm run content && npm run build && npm run deploy",
    "map-electricians": "node scripts/map-outscraper-to-communes.js"
  },
  "keywords": [
    "électricien",
    "urgence",
    "dépannage électrique",
    "île-de-france",
    "oise"
  ],
  "author": "Allo Électricien",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.6.1",
    "gray-matter": "^4.0.3",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.18"
  }
}
